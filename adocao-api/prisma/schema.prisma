// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        String   @id @default(uuid())
  nome      String
  descricao String?
  email     String   @unique
  senha     String
  criacao   DateTime
  estado    STATUS

  @@map("usuarios")
}

enum STATUS {
  ATIVO
  ANALISE
  SUSPENSO
  BANIDO
}
